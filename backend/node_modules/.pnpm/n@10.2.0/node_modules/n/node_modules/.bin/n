#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/run/media/shanto/CurrentBigProjects/authentication-app/backend/node_modules/.pnpm/n@10.2.0/node_modules/n/bin/node_modules:/run/media/shanto/CurrentBigProjects/authentication-app/backend/node_modules/.pnpm/n@10.2.0/node_modules/n/node_modules:/run/media/shanto/CurrentBigProjects/authentication-app/backend/node_modules/.pnpm/n@10.2.0/node_modules:/run/media/shanto/CurrentBigProjects/authentication-app/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/run/media/shanto/CurrentBigProjects/authentication-app/backend/node_modules/.pnpm/n@10.2.0/node_modules/n/bin/node_modules:/run/media/shanto/CurrentBigProjects/authentication-app/backend/node_modules/.pnpm/n@10.2.0/node_modules/n/node_modules:/run/media/shanto/CurrentBigProjects/authentication-app/backend/node_modules/.pnpm/n@10.2.0/node_modules:/run/media/shanto/CurrentBigProjects/authentication-app/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../../bin/n" "$@"
else
  exec bash  "$basedir/../../bin/n" "$@"
fi
